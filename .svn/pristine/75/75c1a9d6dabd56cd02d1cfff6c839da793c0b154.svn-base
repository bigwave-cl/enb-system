@charset 'utf-8';
@import '~@/styles/mixins', '~@/styles/variables';
html {
	font-family: PingFangSC-Regular, Helvetica, STHeiti STXihei, Microsoft JhengHei, Microsoft YaHei, Arial, 'Helvetica Neue', Helvetica, sans-serif;
	font-size: 5.20834vw;
	width: 100%;
	width: 100vw;
	height: 100%;
	height: 100vh;
}

html {
	font-size: 5.20834vw;
	-webkit-text-size-adjust: 100%;
	-ms-text-size-adjust: 100%;
	-moz-text-size-adjust: 100%;
	text-size-adjust: 100%;
}

@media only screen and (min-width:1920px) {
	html {
		font-size: 100px;
	}
}

@media only screen and (max-width:1200px) {
	html {
		font-size: 62.5px;
	}
}

body {
	font-family: inherit;
	font-size: .24rem;
	line-height: 1.4;
	width: 100%;
	width: 100vw;
	height: 100%;
	height: 100vh;
	margin: 0;
	padding: 0;
	color: map-get($color, 300S3);
	background-color: map-get($color, 200);
	/* -webkit-touch-callout: none;
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
	-webkit-overflow-scrolling: touch;
	overflow-scrolling: touch; */
}

body::-webkit-scrollbar {
	display: none;
}

* {
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}

*::before, *::after {
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}

ul {
	margin: 0;
	padding: 0;
	list-style: none;
}

a {
	text-decoration: none;
	color: map-get($color, 400);
}

img {
	max-width: 100%;
	vertical-align: bottom;
	border: none;
}

button, select {
	text-transform: none;
}

.wrapper {
	position: relative;
	overflow: auto;
	/* overflow: hidden;*/
	width: 100%;
	width: 100vw;
	height: 100%;
	height: 100vh;
	&::-webkit-scrollbar {
		display: block;
		width: 10px;
		height: 10px;
		background-color: transparent;
	}
	&::-webkit-scrollbar-track {
		border-radius: 0;
		background-color: map-get($scrollbar, 600);
		/* -webkit-box-shadow: inset 0 0 3px rgba(0, 0, 0, .2);
	    box-shadow: inset 0 0 3px rgba(0, 0, 0, .2); */
	}
	&::-webkit-scrollbar-thumb {
		border-radius: 4px;
		background-color: rgba(map-get($scrollbar, 200), .4);
	}
	&::-webkit-scrollbar-thumb:hover {
		background-color: rgba(map-get($scrollbar, 200), .6);
	}
	&::-webkit-scrollbar-thumb:active {
		background-color: rgba(map-get($scrollbar, 200), 1);
	}
}


/* .wrapper::-webkit-scrollbar {
	display: none;
} */

.boundary {
	width: 100%;
	min-width: 1200px;
	max-width: 1920px;
	margin: 0 auto;
}

.boundary::-webkit-scrollbar {
	display: none;
}

.ask-modal-box.ask-confirm.enb-confirm {
	.ask-modal-wrapper {
		width: 6.2rem;
	}
	.ask-modal-header {
		font-size: .18rem;
		line-height: 1;
		padding: .2rem .42rem;
		color: map-get($color, 100);
		background: map-get($color, 400);
		.ask-modal-title {
			font-size: .18rem;
			font-weight: 900;
			color: map-get($color, 100);
		}
		.ask-close-icon {
			.icon {
				&::after, &::before {
					background: rgba(map-get($color, 100), .8);
				}
				&:hover::after, &:hover::before {
					background: map-get($color, 100);
				}
			}
		}
	}
	.confirm-box {
		font-size: .18rem;
		line-height: 1.4;
		padding: .8rem .14rem;
		text-align: center;
		word-break: break-all;
	}
	.ask-modal-footer {
		padding: 0 .14rem .42rem;
		-webkit-box-pack: center;
		-ms-flex-pack: center;
		justify-content: center;
		-webkit-box-orient: horizontal;
		-webkit-box-direction: normal;
		-ms-flex-flow: row-reverse;
		flex-flow: row-reverse;
		.ask-button {
			line-height: 1;
			width: 1.5rem;
			padding: .12rem 0;
			text-align: center;
			color: map-get($color, 300S2);
			border: 1px solid map-get($color, 100D4);
			border-radius: .04rem;
			background: transparent;
			&+.ask-button {
				margin-right: .7rem;
				color: map-get($color, 100);
				border: 1px solid map-get($color, 400);
				background: map-get($color, 400);
			}
		}
	}
}
.ask-modal-box.custom-enb-confirm {
	.ask-modal-wrapper {
		width: 6.2rem;
		.ask-modal-header {
			padding: .2rem .42rem;
			color: map-get($color, 100);
			background: map-get($color, 400);
			.ask-modal-title {
				font-size: .2rem;
				font-weight: 900;
				line-height: 1;
				color: map-get($color, 100);
			}
			.ask-close-icon {
				.icon {
					&::after, &::before {
						background: rgba(map-get($color, 100), .8);
					}
					&:hover::after, &:hover::before {
						background: map-get($color, 100);
					}
				}
			}
		}
		.ask-modal-body {
			font-size: .16rem;
			.ask-form {
				width: 100%;
				overflow: auto;
				max-height: 70vh;
				padding: .22rem .7rem .3rem;
				.el-radio-group {
					margin-top: 0.12rem;
				}
			}
			.ask-group {
				width: 100%;
				padding-top: .2rem;
				@include flexLayout(flex, normal, top);
				&>.ask-button[type^='submit'] {
					font-size: .18rem;
					line-height: 1;
					display: block;
					width: 4.18rem;
					min-width: auto;
					margin: 0 auto;
					color: map-get($color, 100);
					border-radius: .04rem;
					background: map-get($color, 400);
				}
				label.g-name {
					font-size: .2rem;
					line-height: 1;
					display: block;
					width: 1.4rem;
					height: .44rem;
					vertical-align: middle;
					color: map-get($color, 300);
					-webkit-box-flex: 0;
					-ms-flex: 0 0 1.4rem;
					flex: 0 0 1.4rem;
					@include flexLayout(inline-flex, normal, center);
					i {
						font: inherit;
						display: inline-block;
						width: .2rem;
						height: .08rem;
						text-align: center;
						color: map-get($color, 600);
					}
				}
				.g-value {
					width: calc(100% - 1.4rem);
					-webkit-box-flex: 0;
					-ms-flex: 0 0 calc(100% - 1.4rem);
					flex: 0 0 calc(100% - 1.4rem);
				}
				.g-line {
					position: relative;
					width: 100%;
					@include flexLayout(flex, normal, center);
					-webkit-box-orient: horizontal;
					-webkit-box-direction: reverse;
					-ms-flex-flow: row-reverse;
					flex-flow: row-reverse;
					&.checkbox {
						height: .43rem;
						-webkit-box-orient: horizontal;
						-webkit-box-direction: normal;
						-ms-flex-flow: row;
						flex-flow: row;
					}
					.time-icon {
						font-size: .3rem;
						line-height: 1;
						position: absolute;
						z-index: 2;
						top: 50%;
						right: .1rem;
						overflow: hidden;
						width: .3rem;
						height: .3rem;
						margin-top: -.15rem;
						cursor: pointer;
						color: map-get($color, 400);
						.iconfont {
							font: inherit;
							color: inherit;
						}
					}
					.el-select{
						display: block;
						margin: 0;
						padding: 0;
						width: 100%;
					}
					.el-input__inner{
						font-size: .18rem;
						padding-top: .1rem;
						padding-bottom: .1rem;
						width: 100%;
						height: .43rem;
					}

					&>input:not([type^='checkbox']) {
						font-size: .18rem;
						line-height: 1;
						display: block;
						width: 100%;
						margin: 0;
						padding: .1rem .24rem;
						color: map-get($color, 400);
						border: 1px solid map-get($color, 100D2);
						border-radius: .04rem;
						outline: none;
						background: transparent;
						-webkit-box-flex: 0;
						-ms-flex: 0 0 auto;
						flex: 0 0 auto;
						@include inputPlaceholder(map-get($color, 300S2));
						&:focus {
							-webkit-transition: border .3s;
							transition: border .3s;
							border: 1px solid map-get($color, 400);
						}
						&.error {
							-webkit-transition: border .3s;
							transition: border .3s;
							border: 1px solid map-get($color, 600);
						}
					}
					.ask-button {
						font-size: .18rem;
						line-height: 1;
						width: 1.24rem;
						min-width: auto;
						margin-left: .2rem;
						padding: .12rem 0;
						color: map-get($color, 400);
						border: 1px solid map-get($color, 400);
						border-radius: .04rem;
						-webkit-box-flex: 0;
						-ms-flex: 0 0 1.24rem;
						flex: 0 0 1.24rem;
						&+input {
							width: calc(100% - 1.24rem - .2rem);
							-webkit-box-flex: 0;
							-ms-flex: 0 0 calc(100% - 1.24rem - .2rem);
							flex: 0 0 calc(100% - 1.24rem - .2rem);
						}
					}
				}
				.g-tip {
					font-size: .14rem;
					line-height: 1.4;
					width: 100%;
					min-height: .35rem;
					padding: .15rem 0 0;
					word-break: break-all;
					color: map-get($color, 300S2);
				}
				.g-value.error {
					input {
						-webkit-transition: border .3s;
						transition: border .3s;
						border: 1px solid map-get($color, 600);
					}
					.g-tip {
						-webkit-transition: color .3s;
						transition: color .3s;
						color: map-get($color, 600);
					}
				}
			}
		}
		.ask-modal-footer {
			padding: 0 .14rem .42rem;
			-webkit-box-pack: center;
			-ms-flex-pack: center;
			justify-content: center;
			-webkit-box-orient: horizontal;
			-webkit-box-direction: normal;
			.ask-button {
				line-height: 1;
				width: 1.5rem;
				padding: .12rem 0;
				text-align: center;
				color: map-get($color, 100);
				border: 1px solid map-get($color, 400);
				border-radius: .04rem;
				background: map-get($color, 400);
				&+.ask-button {
					margin-left: .7rem;
				}
				&.gray {
					color: map-get($color, 300S2);
					border: 1px solid map-get($color, 100D4);
					background: transparent;
				}
			}
		}
	}
}

.enb-gray-table {
	.thead-gray {
		font-size: .16rem;
		font-weight: normal;
		line-height: 1;
		padding: .12rem 0;
		text-align: center;
		vertical-align: bottom;
		color: map-get($color, 300S3);
		border-bottom: 2px solid map-get($table, grayThBg);
		background: map-get($table, grayThBg);
	}
	.thead-gray-body-cell {
		font-size: .18rem;
		font-weight: normal;
		line-height: 1.4;
		padding: .08rem .12rem;
		text-align: center;
		color: map-get($color, 300);
		.cell {
			font: inherit;
			color: inherit;
			span {
				font: inherit;
				display: block;
				width: 100%;
				cursor: pointer;
				text-align: center;
				word-break: break-all;
				color: map-get($color, 400);
			}
		}
		.ask-button {
			font-size: .18rem;
			line-height: 1;
			width: .7rem;
			min-width: auto;
			padding: .08rem 0;
			text-align: center;
			color: map-get($color, 700);
			border: 1px solid map-get($color, 700);
			border-radius: .04rem;
			&+.ask-button {
				margin-left: .1rem;
			}
			&.del {
				color: map-get($color, 600S1);
				border: 1px solid map-get($color, 600S1);
			}
		}
	}
	.el-table__body-wrapper {
		&::-webkit-scrollbar {
			display: block;
			width: 10px;
			height: 10px;
			background-color: transparent;
		}
		&::-webkit-scrollbar-track {
			border-radius: 0;
			background-color: map-get($scrollbar, 600);
			/* -webkit-box-shadow: inset 0 0 3px rgba(0, 0, 0, .2);
		    box-shadow: inset 0 0 3px rgba(0, 0, 0, .2); */
		}
		&::-webkit-scrollbar-thumb {
			border-radius: 4px;
			background-color: rgba(map-get($scrollbar, 200), .4);
		}
		&::-webkit-scrollbar-thumb:hover {
			background-color: rgba(map-get($scrollbar, 200), .6);
		}
		&::-webkit-scrollbar-thumb:active {
			background-color: rgba(map-get($scrollbar, 200), 1);
		}
	}
}

.enb-pagination {
	padding: .3rem .06rem;
	text-align: right;
	.el-pager {
		.number {
			font-size: .13rem;
			line-height: .3rem;
			height: .3rem;
			margin: 0 .02rem;
			border: 1px solid map-get($color, 100D1);
			border-radius: .04rem;
			&.active {
				color: map-get($color, 100);
				background: map-get($color, 400);
			}
		}
	}
	.btn-next, .btn-prev {
		font-size: .13rem;
		line-height: .3rem;
		height: .3rem;
		margin: 0 .02rem;
		padding: 0 .06rem;
		border: 1px solid map-get($color, 100D1);
		border-radius: .02rem;
		background: map-get($color, 100S4);
	}
}

.mi-box{
	width: 100%;
	border-radius: .04rem;
	background: map-get($color,100);
	margin: .28rem 0 0 ;
	position: relative;
	.mi-tab-btns{
		position: absolute;
		top: 0;
		right: 0;
		width: 2.8rem;
		height: .6rem;
		z-index: 2;
		@include flexLayout(flex,normal,center);
		.ask-button{
			min-width: auto;
			width: .9rem;
			height: .34rem;
			padding: 0;
			font-size: .18rem;
			color: map-get($color,100);
			background: map-get($color,400);
			border-radius: .04rem;
			@include flexLayout(inline-flex,center,center);
			&+.ask-button{
				margin-left: .16rem;
			}
			&.add{
				color: map-get($color,400);
				background: map-get($color,100);
				border: 1px solid map-get($color,400);
				.iconfont{
					font: inherit;
					color: inherit;
					font-size: .24rem;
					margin-right: .08rem;
				}
			}
		}
	}
	.mi-tabs.el-tabs{
		.el-tabs__header{
			margin: 0;
			padding: 0 2.8rem 0 .2rem;
			&::after{
				content: "";
				position: absolute;
				left: 0;
				bottom: 0;
				width: 100%;
				height: .01rem;
				background-color: map-get($color,100D2);
				z-index: 1;
			}
			.el-tabs__nav-wrap::after{
				display: none;
			}
			.el-tabs__nav-next, .el-tabs__nav-prev{
				line-height: .64rem;
			}
			.el-tabs__item{
				font-size: .18rem;
				line-height: 1;
				color: map-get($color,300S3);
				padding-top: .22rem;
				padding-bottom: .22rem;
				height: .6rem;
				&.is-active{
					color: map-get($color,300);
				}
				&:focus.is-active.is-focus:not(:active){
					box-shadow: none;
				}
			}
			.el-tabs__active-bar{
				background-color: map-get($color,400);
			}
		}
		.mi-tab-label{
			padding: 0 .1rem;
		}
	}
}
